{
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        "packages/site/dist/**",
        "packages/hooks/dist/**",
        "packages/client/dist/**",
        "packages/components/dist/**"
      ]
    },
    "build:staging": {
      "dependsOn": ["^build"],
      "outputs": ["packages/site/dist/**"]
    },
    "start": {
      "cache": false,
      "persistent": true
    }
  }
}
